{
  "name": "abi.js.org",
  "type": "module",
  "version": "0.0.1",
  "description": "Abi Docs.",
  "private": true,
  "engines": {
    "node": ">=20.13",
    "npm": "prefer using pnpm",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.2",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://abi.js.org",
  "author": {
    "name": "Sigui Kess√© Emmanuel",
    "email": "dev@sikessem.com",
    "url": "https://sikessem.com"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "biome ci . && astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "astro dev",
    "fix": "pnpm lint && pnpm format",
    "format": "biome format --write .",
    "inspect": "node --inspect-brk ./node_modules/vite/bin/vite.js --force",
    "lint": "biome check --apply-unsafe .",
    "preview": "astro preview",
    "prod": "pnpm check && astro build",
    "start": "astro dev --open",
    "sync": "astro sync"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.7",
    "@astrojs/deno": "^5.0.1",
    "@astrojs/starlight": "^0.21.1",
    "astro": "^4.5.0",
    "sharp": "^0.33.2",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.0",
    "@types/bun": "^1.0.8"
  },
  "trustedDependencies": ["sharp"]
}
