{
  "name": "@velox-sh/velox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A small, swift, simple and secure solution for streamlined full-stack web applications.",
  "engines": {
    "bun": ">=1.0.0",
    "npm": "prefer using bun",
    "pnpm": "prefer using bun",
    "yarn": "prefer using bun",
    "node": "prefer using bun"
  },
  "packageManager": "bun@1.0.21",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/velox-sh#readme",
  "author": {
    "name": "Sigui Kess√© Emmanuel",
    "email": "contact@sigui.ci",
    "url": "https://sigui.ci"
  },
  "scripts": {
    "check": "biome ci .",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "bun --bun vite",
    "fix": "bun lint && bun format",
    "format": "biome format --write .",
    "lint": "biome check --apply-unsafe .",
    "build": "tsc && vite build",
    "preview": "bun --bun vite preview",
    "start": "bun --bun vite --open"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@builder.io/qwik": "^1.3.0",
    "bun-types": "^1.0.21",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
