{
  "organizeImports": {
    "ignore": ["tmp", "node_modules", "dist"]
  },
  "linter": {
    "ignore": ["tmp", "node_modules", "dist"],
    "rules": {
      "style": {
        "noNonNullAssertion": "off"
      },
      "complexity": {
        "noBannedTypes": "off"
      },
      "suspicious": {
        "noExplicitAny": "off",
        "noConfusingVoidType": "off"
      }
    }
  },
  "formatter": {
    "ignore": ["tmp", "node_modules", "dist"]
  }
}
