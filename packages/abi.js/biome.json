{
  "organizeImports": {
    "ignore": ["node_modules", "jsr", "js", "ts"]
  },
  "linter": {
    "ignore": ["node_modules", "jsr", "js", "ts"],
    "rules": {
      "style": {
        "noNonNullAssertion": "off"
      },
      "complexity": {
        "noBannedTypes": "off"
      },
      "suspicious": {
        "noExplicitAny": "off"
      }
    }
  },
  "formatter": {
    "ignore": ["node_modules", "jsr", "js", "ts"]
  }
}
